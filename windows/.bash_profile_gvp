alias gvp-git='cd c:\\Dev\\agilecontent\\gvp'

alias gvp-install='./Install.cmd'

alias gvp-prepare='.\\gvpbuilder.cmd; .\\gvp.cmd prepare'
alias gvp-build='.\\gvp.cmd build'
alias gvp-build-debug='.\\gvp.cmd build -debug'
alias gvp-prepare-build='gvp-prepare; gvp-build'
alias gvp-package='.\\gvp.cmd package -all -env:local -nb'
alias gvp-prepare-build-package='gvp-prepare; gvp-build; gvp-package'
alias gvp-migration-up='powershell -ExecutionPolicy RemoteSigned -File "Install.ps1" "UP"'
alias gvp-migration-down='powershell -ExecutionPolicy RemoteSigned -File "Install.ps1" "DOWN"'

alias gvp-mib3-git='cd c:\\Dev\\agilecontent\\mib\\mib3'
alias gvp-mib3-server-update='Server\\update_mib_server.cmd'
alias gvp-mib3-watcher='./MibWatcher.cmd'
alias gvp-prepare-build-server-update='gvp-prepare; gvp-build; gvp-mib3-server-update'
alias gvp-prepare-build-package-server-update='gvp-prepare-build-server-update; gvp-package'